{
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "never"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "eslint.format.enable": true,
  "eslint.ignoreUntitled": true,
  "eslint.run": "onSave",
  "eslint.validate": ["javascript", "typescript"],

  "javascript.preferences.importModuleSpecifier": "non-relative",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "[dockerfile]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },

  "json.format.enable": true,
  "workbench.colorCustomizations": {
    "editorWarning.background": "#ff0000",
    "activityBar.activeBackground": "#59b5cd",
    "activityBar.background": "#59b5cd",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#ab3493",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#59b5cd",
    "statusBar.background": "#38a0bb",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#2c7e94",
    "statusBarItem.remoteBackground": "#38a0bb",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#38a0bb",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#38a0bb99",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "peacock.color": "#38a0bb"
}
